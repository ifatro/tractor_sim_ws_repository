<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find gazebo_tractor_sim_pkg)/urdf/my_robot.gazebo" />

  <link name="robot_footprint"></link>

   <joint name="robot_footprint_joint" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="robot_footprint"/>
      <child link="chassis" />
   </joint>


   <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="12.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="1 .5 .2"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="1 .5 .2"/>
      </geometry>
    </visual>

  </link>


  <link name='left_wheel'>

    <collision name='left_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </collision>

    <inertial name='left_wheel_inertial'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <mass value="5.0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <visual name='left_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </visual>

  </link>

  <link name='right_wheel'>

    <collision name='right_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </collision>

    <inertial name='right_wheel_inertial'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <mass value="5.0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <visual name='right_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </visual>

  </link>

  <link name='left_wheel_2'>

    <collision name='left_wheel_2_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </collision>

    <inertial name='left_wheel_2_inertial'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <mass value="5.0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <visual name='left_wheel_2_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </visual>

  </link>

  <link name='right_wheel_2'>
    <collision name='right_wheel_2_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </collision>

    <inertial name='right_wheel_2_inertial'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <mass value="5.0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <visual name='right_wheel_2_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.09"/>
      </geometry>
    </visual>

  </link>

  <link name='camera_shaft'>

    <collision name='camera_shaft_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.01" length="0.25"/>
      </geometry>
    </collision>

    <inertial name='camera_shaft_inertial'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <visual name='camera_shaft_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.01" length="0.25"/>
      </geometry>
    </visual>

  </link>

  <link name='camera'>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.1"/>
      <box_inertia m="0.1" x="0.05" y="0.05" z="0.05"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".05 .1 .05"/>
      </geometry>
    </visual>

    <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size=".05 .1 .05"/>
      </geometry>
    </collision>
  </link>


  <!-- Joints -->
  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0.37 0.31 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="0.75"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0.37 -0.31 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="0.75"/>
  </joint>

  <joint type="continuous" name="left_wheel_2_hinge">
    <origin xyz="-0.37 0.31 0" rpy="0 0 0"/>
    <child link="left_wheel_2"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="0.75"/>
  </joint>

  <joint type="continuous" name="right_wheel_2_hinge">
    <origin xyz="-0.37 -0.31 0" rpy="0 0 0"/>
    <child link="right_wheel_2"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="0.75"/>
  </joint>

  <joint type="continuous" name="camera_shaft_hinge">
    <origin xyz="0.22 0 .1" rpy="0 0 0"/>
    <child link="camera_shaft"/>
    <parent link="chassis"/>
    <axis xyz="0 0 1" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="0.75"/>
  </joint>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.0 0 .12" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="camera_shaft"/>
    <axis xyz="0 1 0"/>
  </joint>


  <!-- transmission -->
  <transmission name="chassis_to_wheel_1_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="motor1">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="right_wheel_hinge">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="chassis_to_wheel_2_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="motor2">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="left_wheel_hinge">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="chassis_to_wheel_3_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="motor3">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="right_wheel_2_hinge">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="chassis_to_wheel_4_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="motor4">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="left_wheel_2_hinge">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <transmission name="chassis_to_camera_shaft_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="motor5">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="camera_shaft_hinge">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
  </transmission>


</robot>
